auto-reload yes
reload-time 60
browser "open %u"
mark-as-read-on-hover no
max-items 100
show-read-articles no

macro y set browser "echo %u | pbcopy"; open-in-browser; set browser "open %u"
macro 2 set browser "echo %u | pbcopy"; two; set browser "open %u"

bind-key j next
bind-key k prev

color listfocus color161 color234 bold
color listfocus_unread color161 color234 bold
color listnormal color252 black
color listnormal_unread color252 black
color info color238 color253
color background color252 black

urls-source "inoreader"
inoreader-flag-star "b"
